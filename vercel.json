{
  "version": 2,
  "name": "estudio-artesana",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/pages",
      "destination": "/pages/tienda",
      "permanent": true
    },
    {
      "source": "/pages/tienda/(.*)",
      "destination": "/tienda/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/tienda",
      "destination": "/tienda/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.js$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
